{
  "name": "eli-auth-provider-router",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "bootstrap": "lerna exec yarn install",
    "package-install": "cd ./examples && yarn install --production",
    "examples-install": "cd ./packages/@core/auth-provider-router && yarn install --production",
    "start": "lerna run  --parallel start",
    "build:watch": "lerna run build:watch --parallel",
    "build": "lerna run --parallel build",
    "server": "cd ./examples && yarn server",
    "heroku-postbuild": "yarn install && yarn run bootstrap"
  },
  "dependencies": {
    "lerna": "^6.5.1"
  },
  "workspaces": [
    "./packages/@core/auth-provider-router",
    "./examples"
  ]
}
