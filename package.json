{
  "name": "eli-auth-provider-router",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "bootstrap": "lerna exec yarn install",
    "start": "lerna run  --parallel start",
    "build:watch": "lerna run build:watch --parallel",
    "build:docker": "cd ./examples && yarn build:docker",
    "build": "cd ./examples && yarn build",
    "build:package": "cd ./packages/@core/auth-provider-router && yarn build",
    "build:prod": "yarn build:package && cd ./examples && yarn build:prod",
    "server": "cd ./examples && yarn server"
  },
  "dependencies": {
    "lerna": "^6.5.1"
  },
  "workspaces": [
    "./packages/@core/auth-provider-router",
    "./examples"
  ]
}
